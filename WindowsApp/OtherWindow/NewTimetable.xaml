<Window x:Class="WindowsApp.OtherWindow.NewTimetable"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:WindowsApp.OtherWindow"
        mc:Ignorable="d"
        Title="Новый файл" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <TextBox x:Name="TextBox"
                 Margin="10" Grid.Column="0"/>
        <Button Content="Ok"
                Margin="10" Grid.Row="1" Grid.Column="0"
                x:Name="GetTextButton" Click="GetTextButton_OnClick"/>
    </Grid>
</Window>
